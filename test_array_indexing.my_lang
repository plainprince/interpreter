# Test Array Indexing

# Test basic array creation and indexing
arr = [10, 20, 30, 40, 50];
console.log("Array:", arr);
console.log("arr[0]:", arr[0]);
console.log("arr[2]:", arr[2]);
console.log("arr[4]:", arr[4]);

# Test array length
console.log("Array length:", arr.length);

# Test modifying array elements
arr[1] = 99;
console.log("After arr[1] = 99:", arr);

# Test nested arrays
nested = [[1, 2], [3, 4], [5, 6]];
console.log("Nested array:", nested);
console.log("nested[0]:", nested[0]);
console.log("nested[1][0]:", nested[1][0]);
console.log("nested[2][1]:", nested[2][1]);

# Test array in loop
console.log("Loop through array:");
for (i, arr.length);
    console.log("Index", i, "value:", arr[i]);
end;

# Test dynamic array operations
console.log("Testing push operation:");
arr.push(60);
console.log("After push(60):", arr);
console.log("New length:", arr.length);

console.log("All array indexing tests completed!");
